bool isPalindrome(int x) {
    if(x<0){
        return false;
    }
    if (x < 0) {
            return false;
        }
        int original = x;
        long long sum = 0;
        while(x!=0){
           int y = x%10;
            sum = sum*10 + y;
            x = x/10;
        }
        if(sum == original){
            return true;
        }
        return  false;
    
}
